---
import { getCollection } from "astro:content";
import ContactTemplate from "../../layouts/ContactTemplate.astro";

export async function getStaticPaths() {
	const contact = await getCollection("contact");
	return contact.map((contact) => ({
		params: { slug: contact.slug },
		props: contact,
	}));
} 

const contactData = Astro.props;
const { Content } = await contactData.render();
---
<ContactTemplate {...contactData.data}>
	<Content />
</ContactTemplate>